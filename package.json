{
  "name": "albertmolodec.github.io",
  "version": "2.0.0",
  "description": "My personal site",
  "author": "Albert Abdulmanov <albert.abdu@gmail.com>",
  "homepage": "https://github.com/albertmolodec/albertmolodec.github.io",
  "scripts": {
    "clean": "run-s clean:*",
    "clean:cache": "rimraf ./.cache",
    "clean:dist": "rimraf ./dist",
    "clean:bundle": "rimraf ./bundle",
    "build": "cross-env ELEVENTY_ENV=production run-s clean:* build:assets build:html",
    "build:html": "eleventy",
    "build:assets": "parcel build ./src/assets/css/app.pcss ./src/assets/js/app.js --dist-dir ./bundle --no-source-maps",
    "watch": "cross-env ELEVENTY_ENV=development run-s clean:* build:assets && run-p serve watch:*",
    "serve": "eleventy --serve --port=8080",
    "watch:assets": "parcel watch ./src/assets/css/app.pcss ./src/assets/js/app.js --dist-dir ./bundle --no-source-maps --no-hmr",
    "dev": "yarn watch",
    "update:deps": "yarn upgrade-interactive --latest && yarn upgrade",
    "update:cssdb": "npx browserslist@latest --update-db",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:css": "stylelint src/**/*.css",
    "lint": "yarn lint:js && yarn lint:css",
    "format": "prettier --trailing-comma es5 --single-quote --write \"src/**/*.js\" \"src/**/*.md\""
  },
  "dependencies": {
    "@11ty/eleventy": "0.11.0",
    "@11ty/eleventy-plugin-rss": "1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.1",
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@tbranyen/jsdom": "13.0.0",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "eleventy-plugin-pwa": "1.0.8",
    "html-minifier": "4.0.0",
    "markdown-it": "11.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.1",
    "postcss-preset-env": "6.7.0",
    "postcss-reporter": "6.0.1",
    "rimraf": "3.0.2",
    "slugify": "1.4.4",
    "turbolinks": "5.2.0",
    "what-input": "5.2.10"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-alias": "^1,.1.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "stylelint": "^12.0.1",
    "stylelint-a11y": "^1.2.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.1.1"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albertmolodec/albertmolodec.github.io.git"
  },
  "keywords": [
    "Albert Abdulmanov",
    "personal",
    "portfolio",
    "blog",
    "frontend",
    "development"
  ]
}
